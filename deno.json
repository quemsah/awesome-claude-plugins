{
  "nodeModulesDir": "auto",
  "unstable": ["unsafe-proto", "sloppy-imports"],
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "strict": true,
    "jsx": "preserve"
  },
  "tasks": {
    "install": "deno install --allow-scripts",
    "dev": "deno run -A --unstable-detect-cjs npm:next@latest dev",
    "build": "deno run -A --unstable-detect-cjs npm:next@latest build",
    "start": "deno run -A --unstable-detect-cjs npm:next@latest start",
    "style": "deno run -A npm:@biomejs/biome format --write .; deno lint --fix; deno run -A npm:@biomejs/biome lint --write ."
  },
  "imports": {
    "@/": "./",
    "npm:@octokit/openapi-types": "npm:@octokit/openapi-types@^27.0.0",
    "npm:@radix-ui/react-avatar": "npm:@radix-ui/react-avatar@^1.1.11",
    "npm:@radix-ui/react-dropdown-menu": "npm:@radix-ui/react-dropdown-menu@2.1.16",
    "npm:@radix-ui/react-icons": "npm:@radix-ui/react-icons@1.3.2",
    "npm:@radix-ui/react-select": "npm:@radix-ui/react-select@^2.2.6",
    "npm:@radix-ui/react-separator": "npm:@radix-ui/react-separator@^1.1.8",
    "npm:@radix-ui/react-slot": "npm:@radix-ui/react-slot@^1.2.4",
    "npm:class-variance-authority": "npm:class-variance-authority@^0.7.1",
    "npm:clsx": "npm:clsx@^2.1.1",
    "npm:lucide-react": "npm:lucide-react@^0.556.0",
    "npm:next": "npm:next@16.0.7",
    "npm:next-themes": "npm:next-themes@0.4.6",
    "npm:react": "npm:react@19.2.1",
    "npm:react-dom": "npm:react-dom@19.2.1",
    "npm:recharts": "npm:recharts@3.5.1",
    "npm:tailwind-merge": "npm:tailwind-merge@^3.4.0",
    "npm:@biomejs/biome": "npm:@biomejs/biome@^2.3.8",
    "npm:@tailwindcss/postcss": "npm:@tailwindcss/postcss@^4.1.17",
    "npm:@types/node": "npm:@types/node@^24.10.1",
    "npm:@types/react": "npm:@types/react@^19.2.7",
    "npm:@types/react-dom": "npm:@types/react-dom@^19.2.3",
    "npm:husky": "npm:husky@^9.1.7",
    "npm:tailwindcss": "npm:tailwindcss@^4.1.17",
    "npm:typescript": "npm:typescript@^5.9.3"
  }
}
